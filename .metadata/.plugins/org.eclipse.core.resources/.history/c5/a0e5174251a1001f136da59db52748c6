package tema3FJRJ;

public class Repaso {

	public static void main(String[] args) {

		int mensual = 10;
		int pagoFinal= 0;
		
		for (int i = 0; i > 20; i++) {
			System.out.printf("El mes %d pagara %d \n", i+1,mensual);
			pagoFinal = pagoFinal + mensual;
		}
		
		System.out.println(pagoFinal);
		
		
		
	}

}

//10,11,13,14,16, cajafuerte, vladi, mbappe