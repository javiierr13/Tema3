package tema3FJRJ;

import java.util.Iterator;
import java.util.Scanner;

public class Repaso {

	public static void main(String[] args) {

		int pagoMensual = 10;
		int dineroFinal = 0;
		for (int i = 0; i < 20; i++) {
			System.out.printf("El mes %d debera pagar %d \n", i+1, pagoMensual);
			
		}
		System.out.printf("En total pagara %d \n", dineroFinal);
	}
}

